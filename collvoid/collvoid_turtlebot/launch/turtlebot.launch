<launch>
  <arg name="master" />

  <node pkg="multimaster" type="master_sync.py" name="master_sync" output="screen"> 
    <param name="foreign_master" value="http://$(arg master):11311" />      
    <rosparam command="load" file="$(find collvoid_turtlebot)/params/config.yaml" />
  </node>

  <include file="$(find udp_multicast)/receiver.launch" />

  <include file="$(find collvoid_turtlebot)/launch/turtlebot_master.launch" />

</launch>
